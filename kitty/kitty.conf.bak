font_family FiraCode Nerd Font Mono Reg
bold_font auto
italic_font auto
bold_italic_font auto
font_size 12
cursor_shape underline
cursor_underline_thickness 1.5
mouse_hide_wait 3.0
open_url_with default
url_style curly
url_prefixes file ftp ftps gemini git gopher http https irc ircs kitty mailto news sftp ssh tcp udp cmd
detect_urls yes
copy_on_select no
enable_audio_bell no

#foreground #CDD6F4
#background #1F1D29
#background_opacity 0.90
#background_blur 0

